{
    "name": "casbin/dbal-adapter",
    "keywords": [
        "casbin",
        "database",
        "dbal",
        "rbac",
        "permission",
        "acl",
        "adapter"
    ],
    "description": "Database Abstraction Layer adapter for php-casbin. ",
    "authors": [
        {
            "name": "TechLee",
            "email": "techlee@qq.com"
        }
    ],
    "license": "Apache-2.0",
    "require": {
        "casbin/casbin": "^3.0",
        "doctrine/dbal": "^2.9|^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|~8.0",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "CasbinAdapter\\DBAL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CasbinAdapter\\DBAL\\Tests\\": "tests/"
        }
    }
}